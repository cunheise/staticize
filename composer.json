{
  "name": "staticize/staticize",
  "type": "library",
  "description": "Put php outputbuffer into cache",
  "keywords": [
    "static",
    "outputbuffer",
    "page cache"
  ],
  "homepage": "https://github.com/cunheise/staticize.git",
  "license": "MIT",
  "authors": [
    {
      "name": "Nathan Wu",
      "email": "nathan@joomlacreator.com",
      "homepage": "http://www.joomlacreator.com",
      "role": "Developer"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "pimple/pimple": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Staticize\\": "src/Staticize"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "require": {
    "symfony/cache": "^3.4"
  }
}